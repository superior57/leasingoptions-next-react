{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState, useEffect } from 'react';\nimport style from './birthday-countdown.module.scss';\nimport { ContentService } from 'business-logic/contentService';\nimport FlipNumbers from 'react-flip-numbers';\n\nconst BirthdayCountdown = ({\n  className\n}) => {\n  const {\n    0: countTotal,\n    1: setCountTotal\n  } = useState('0000000000');\n  useEffect(() => {\n    updateTotal();\n    const interval = setInterval(async () => {\n      await updateTotal();\n    }, 60000);\n    return () => clearInterval(interval);\n  }, []);\n\n  async function updateTotal() {\n    ContentService.GetTotalLeasedValues().then(function (data) {\n      let updatedTotal = Math.floor(data.data.total).toString();\n\n      if (countTotal !== updatedTotal) {\n        setCountTotal(updatedTotal);\n      }\n    });\n  }\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    className: `${style['birthday-countdown']} order-md-3 ${className}`,\n    children: [/*#__PURE__*/_jsx(\"p\", {\n      children: \"So far we have leased\"\n    }), /*#__PURE__*/_jsx(FlipNumbers, {\n      height: 60,\n      width: 45,\n      color: \"#3e3e40\",\n      background: \"#fdfdfd\",\n      play: true,\n      duration: 3,\n      perspective: 0,\n      numbers: countTotal\n    }), /*#__PURE__*/_jsx(\"p\", {\n      children: \"worth of vehicles\"\n    })]\n  });\n};\n\nexport default BirthdayCountdown;","map":null,"metadata":{},"sourceType":"module"}