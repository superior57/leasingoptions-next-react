{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useEffect, useState } from 'react';\nimport Link from 'next/link';\nimport style from './search.module.scss';\nimport { VehicleTypes } from 'enums/VehicleTypes';\nimport { GetDealTypeUrl } from '../../helpers/urlHelper';\nimport { LeasingDealTypes } from '../../enums/LeasingDealTypes';\nimport { GetModels } from 'business-logic/vehicleService';\n\nvar SearchModels = function SearchModels(_ref) {\n  var active = _ref.active,\n      makes = _ref.makes,\n      handleMakeUpdate = _ref.handleMakeUpdate,\n      currentMake = _ref.currentMake,\n      dealToggle = _ref.dealToggle;\n\n  var _useState = useState([]),\n      modelList = _useState[0],\n      setModelList = _useState[1];\n\n  useEffect(function () {\n    setModelList(makes);\n\n    if (currentMake) {\n      GetModels(dealToggle === LeasingDealTypes.Van ? VehicleTypes.Commercial : VehicleTypes.Car, currentMake).then(function (data) {\n        setModelList(data.data);\n      });\n    }\n  }, [makes, currentMake]);\n  return /*#__PURE__*/_jsx(\"div\", {\n    className: \"\".concat(style['modal-panel'], \" \").concat(active ? style['is-active'] : '', \" flex-wrap align-items-center justify-content-start\"),\n    children: /*#__PURE__*/_jsx(\"ul\", {\n      children: modelList.map(function (model, index) {\n        if (currentMake) {\n          return /*#__PURE__*/_jsx(\"li\", {\n            children: /*#__PURE__*/_jsx(Link, {\n              passHref: true,\n              href: GetDealTypeUrl(dealToggle, '/' + currentMake + '/' + model.id),\n              children: /*#__PURE__*/_jsx(\"a\", {\n                className: \"text-decoration-none\",\n                children: model.text\n              })\n            })\n          }, index);\n        } else {\n          return /*#__PURE__*/_jsxs(\"li\", {\n            onClick: function onClick(evt) {\n              return handleMakeUpdate(model.id);\n            },\n            children: [model.text, \" Models\"]\n          }, index);\n        }\n      })\n    })\n  });\n};\n\nexport default SearchModels;","map":null,"metadata":{},"sourceType":"module"}