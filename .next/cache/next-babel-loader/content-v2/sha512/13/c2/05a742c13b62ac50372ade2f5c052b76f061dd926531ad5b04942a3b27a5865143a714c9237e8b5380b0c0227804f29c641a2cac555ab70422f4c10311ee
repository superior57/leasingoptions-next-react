{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useContext } from 'react';\nimport { Tabs, Tab } from 'react-bootstrap';\nimport ModalContext from '../../HOCs/modal/modalContext';\nimport ModalGallery from '../../layout/modal-gallery';\n\nconst ModelTabs = ({\n  modalTitle,\n  activeTab,\n  handleActiveTab,\n  tabOne,\n  tabTwo = null,\n  tabThree = null,\n  tabFour = null\n}) => {\n  const modalContext = useContext(ModalContext);\n\n  const handleGalleryClick = (imagePath, imageAlt) => {\n    modalContext.showModal(ModalGallery, {\n      show: true,\n      title: modalTitle,\n      imagePath: imagePath,\n      imageAlt: imageAlt\n    });\n  };\n\n  const handleTabChange = key => handleActiveTab(key);\n\n  return /*#__PURE__*/_jsxs(Tabs, {\n    id: \"model-tabs\",\n    activeKey: activeTab,\n    onSelect: handleTabChange,\n    children: [tabOne && /*#__PURE__*/_jsx(Tab, {\n      eventKey: \"review\",\n      title: \"Review\",\n      dangerouslySetInnerHTML: {\n        __html: tabOne.review\n      }\n    }), tabTwo.length > 0 && /*#__PURE__*/_jsx(Tab, {\n      eventKey: \"gallery\",\n      title: \"Gallery\",\n      children: /*#__PURE__*/_jsx(\"div\", {\n        className: \"row\",\n        children: tabTwo === null || tabTwo === void 0 ? void 0 : tabTwo.map((image, index) => /*#__PURE__*/_jsx(\"div\", {\n          className: \"col-12 col-sm-6 col-md-8 col-lg-6 col-xl-4 pb-4\",\n          children: /*#__PURE__*/_jsx(\"img\", {\n            \"data-src\": image,\n            className: \"img-fluid lazyload\",\n            alt: \"Gallery thumbnail\",\n            onClick: () => {\n              handleGalleryClick(image, \"\");\n            }\n          })\n        }, `thumbnail-${index}`))\n      })\n    })]\n  });\n};\n\nexport default ModelTabs;","map":null,"metadata":{},"sourceType":"module"}